<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO" name="Immunization">
  <Properties>
    <Property name="filename">./logs/immunization.log</Property>
  </Properties>
  <Appenders>
    <File name="vaccinestatus" fileName="logs/Vaccine-Status.log">
      <PatternLayout>
        <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root level="info" additivity="true">
      <AppenderRef ref="vaccinestatus"/>
    </Root>
  </Loggers>
</Configuration>

